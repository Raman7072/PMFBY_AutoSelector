{
  "manifest_version": 3,
  "name": "Auto Checkbox Selector",
  "version": "1.2",
  "description": "Select 99 checkboxes starting from a given number.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Start Selecting Checkboxes",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
